   "source": [
    "def computeCost(xdata, theta, y):\n",
    "    h = np.dot(xdata, theta)\n",
    "    error_sum = np.sum((h - y)**2)\n",
    "    cost = error_sum / (2 * y.shape[0])\n",
    "    return cost"
   ]
  }
